<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent">
	<LinearLayout android:layout_width="match_parent"
		android:layout_height="match_parent" android:background="@color/white"
		 android:orientation="vertical">
		<RelativeLayout android:id="@+id/obj_info_tab" style="@style/title_bar"
			android:layout_width="fill_parent" android:layout_height="wrap_content">
			<ImageButton android:id="@+id/obj_info_back"  android:visibility="gone"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_centerVertical="true" android:background="@drawable/button_background_back" />
			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:layout_centerInParent="true"
				android:text="@string/ei_map_title" android:textColor="@color/white"
				android:textSize="24sp" />
		</RelativeLayout>
		<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
			android:orientation="vertical" android:layout_width="fill_parent"  android:id="@+id/map_all"
			android:layout_height="fill_parent" android:background="@drawable/map_all">
			<ImageView android:layout_width="fill_parent" android:id="@+id/map_c"  
				android:layout_height="fill_parent" />
		</FrameLayout>
	</LinearLayout>
  <erb.unicomedu.ui.MenuControl
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>